#: src/sdcv.cpp:127
#, c-format
msgid ""
"Unknown option.\n"
"Try '%s --help' for more information.\n"
msgstr ""
"Неизвестная опция.\n"
"Попробуйте '%s --help' для получения более подробной информации.\n"

#: src/sdcv.cpp:134
#, c-format
msgid "Usage: %s [OPTIONS] words\n"
msgstr "Использование: %s [ОПЦИИ] слова\n"

#: src/sdcv.cpp:135
#, c-format
msgid "-h, --help               display this help and exit\n"
msgstr "-h, --help               показать это сообщение и завершить работу\n"

#: src/sdcv.cpp:136
#, c-format
msgid "-v, --version            display version information and exit\n"
msgstr "-v, --version            показать номер версии и завершить работу\n"

#: src/sdcv.cpp:137
#, c-format
msgid ""
"-l, --list-dicts         display list of available dictionaries and exit\n"
msgstr ""
"-l, --list-dicts         показать список доступных словарей и завершить "
"работу\n"

#: src/sdcv.cpp:138
#, c-format
msgid ""
"-u, --use-dict bookname  for search use only dictionary with this bookname\n"
msgstr ""
"-u, --use-dict имя словаря  для поиска использовать только этот словарь\n"

#: src/sdcv.cpp:139
#, c-format
msgid "-n, --non-interactive    for use in scripts\n"
msgstr "-n, --non-interactive    для использования в 'скриптах'\n"

#: src/sdcv.cpp:140
#, c-format
msgid "--utf8-output            output must be in utf8\n"
msgstr "--utf8-output            вывод программы должен быть в utf8\n"

#: src/sdcv.cpp:141
#, c-format
msgid "--utf8-input             input of sdcv in utf8\n"
msgstr "--utf8-input             ввод программы в utf8\n"

#: src/sdcv.cpp:142
#, c-format
msgid ""
"--data-dir path/to/dir   use this directory as path to stardict data "
"directory\n"
msgstr ""
"--data-dir директория    использовать эту директорию в качестве пути к "
"stardict data directory\n"

#: src/sdcv.cpp:148
#, c-format
msgid "Console version of Stardict, version %s\n"
msgstr "Консольная версия StarDict, номер версии %s\n"

#: src/sdcv.cpp:169
#, c-format
msgid "Dictionary's name   Word count\n"
msgstr "Название словаря     Количество слов\n"

#: src/sdcv.cpp:191
#, c-format
msgid "g_mkdir failed: %s\n"
msgstr "g_mkdir завершился с ошибкой: %s\n"

#: src/sdcv.cpp:207
msgid "Enter word or phrase: "
msgstr "Введите слово или фразу: "

#: src/sdcv.cpp:215
#, c-format
msgid "There are no words/phrases to translate.\n"
msgstr "Не задано слова/фразы для перевода.\n"

#: src/libwrapper.cpp:246
msgid "popen failed"
msgstr "функция popen завершилась с ошибкой"

#: src/libwrapper.cpp:283
#, c-format
msgid "Can not convert %s to utf8.\n"
msgstr "Не могу преобразовать %s в utf8.\n"

#: src/libwrapper.cpp:340 src/libwrapper.cpp:372
#, c-format
msgid "Found %d items, similar to %s.\n"
msgstr "Найдено %d слов, похожих на %s.\n"

#: src/libwrapper.cpp:353
#, c-format
msgid "Your choice[-1 to abort]: "
msgstr "Ваш выбор[-1 - отмена]:"

#: src/libwrapper.cpp:367
#, c-format
msgid ""
"Invalid choice.\n"
"It must be from 0 to %d or -1.\n"
msgstr ""
"Неправильный выбор.\n"
"Должно быть от 0 до %d или -1.\n"

#: src/libwrapper.cpp:383
#, c-format
msgid "Nothing similar to %s, sorry :(\n"
msgstr "Ничего похожего на %s, извините :(\n"

#~ msgid "Can not convert %s to current locale.\n"
#~ msgstr "Не могу преобразовать %s в локальную кодировку.\n"

#~ msgid "There is no dictionary with this bookname: %s.\n"
#~ msgstr "Словаря с таким именем не существует: %s.\n"
